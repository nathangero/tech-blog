{{!-- Show all the user's post in the Dashboard --}}
<section class="my-3">
  <div id="dashboard-posts">
    {{#each posts as |post|}}
    {{> dashboard-posts}}
    {{/each}}

    <button id="button-make-post" class="button m-3">Create Post</button>
  </div>

  {{> new-post}}

</section>

<script src="./js/post.js"></script>